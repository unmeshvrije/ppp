#!/bin/bash

NP=1

if [ -z $NP ];
then
       echo "Usage: ./test Numer_of_processors"
       exit 1
fi

prun -v -np $NP     ../asp-seq ../../sample_graphs/random500.gr.new > random.$NP.time 2> random.err
mv seqmat.out ../../aspseq/out/random500.out

prun -v -np $NP     ../asp-seq ../../sample_graphs/random1000.gr.new >> random.$NP.time 2> random.err
  mv seqmat.out ../../aspseq/out/random1000.out

prun -v -np $NP     ../asp-seq ../../sample_graphs/random2000.gr.new >> random.$NP.time 2> random.err
  mv seqmat.out ../../aspseq/out/random2000.out

prun -v -np $NP     ../asp-seq ../../sample_graphs/random3000.gr.new >> random.$NP.time 2> random.err
  mv seqmat.out ../../aspseq/out/random3000.out

prun -v -np $NP     ../asp-seq ../../sample_graphs/random4000.gr.new >> random.$NP.time 2> random.err
  mv seqmat.out ../../aspseq/out/random4000.out
